LCDwrchar(OnOrOff ? 0x00:'/');
}

CurrentSelect=~CurrentSelect;
}
while(PORTAbits.RA4 || PORTAbits.RA1);
}
if(PORTAbits.RA0) {
_delay((unsigned long)((12)*(0x7A1200/4000.0)));
