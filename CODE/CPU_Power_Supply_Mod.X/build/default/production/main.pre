CurrentSelect=0;
Last=0;
LATBbits.LATB7=1;
LATBbits.LATB1=1;
LATBbits.LATB6=1;
LATBbits.LATB0=1;
LCDsetaddr(0x00);
LCDwrstring("All Voltages:/  ");
LCDsetaddr(0x40);
LCDwrstring("            MENU");
while(1) {
